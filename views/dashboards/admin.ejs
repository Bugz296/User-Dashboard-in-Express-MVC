<%- include('../partials/navbar') %>
        <div class="container">
            <h3>Manage Users</h3>
            <a href="/users/new" class="pull-right"><h4>ADD USER</h4></a>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Created At</th>
                        <th>User Level</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
<%           for(let field of list_of_users){ %>
                    <tr>
                        <td><%= field.id %></td>
                        <td><a href="/users/show/<%= field.id %>"><%= field.first_name+" " +field.last_name %></a></td>
                        <td><%= field.email %></td>
                        <td><%= field.created_at %></td>
                        <td><%= field.user_level %></td>
                        <td><a href="/users/edit/<%= field.id %>">edit</a> | <a href="/users/delete/<%= field.id %>">remove</a></td>
                    </tr>
<%           } %>
                </tbody>
            </table>
		</div>
	</body>
</html>